{
  "name": "times-square-ui",
  "version": "0.0.1",
  "description": "Front end for Times Square, parameterized Jupyter Notebooks as websites",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,json,css,graphql,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,json,css,graphql,yaml}\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lsst-sqre/times-square-ui.git"
  },
  "author": "Jonathan Sick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lsst-sqre/times-square-ui/issues"
  },
  "homepage": "https://github.com/lsst-sqre/times-square-ui#readme",
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ]
  },
  "dependencies": {
    "next": "^12.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.{js,json,css,graphql,yaml}": "prettier --write"
  }
}
